<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Stack Overflow</os:ShortName>
<os:Description>Search among stackoverflow.com questions and answers</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/stackoverflow.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/stackoverflow.ico</IconUpdateUrl>
<SearchForm>http://stackoverflow.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://stackoverflow.com/search?q={searchTerms}">
</os:Url>
</SearchPlugin>