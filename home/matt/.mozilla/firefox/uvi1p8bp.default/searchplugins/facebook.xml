<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Facebook</os:ShortName>
<os:Description>Search Facebook</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/facebook.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/facebook.ico</IconUpdateUrl>
<SearchForm>http://www.facebook.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.facebook.com/search/?q={searchTerms}&amp;src=os">
</os:Url>
</SearchPlugin>