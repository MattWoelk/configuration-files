<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Mycroft Project</os:ShortName>
<os:Description>Mycroft Project: Sherlock &amp; OpenSearch Search Engine Plugins</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/opensearch.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/favicon.ico</IconUpdateUrl>
<SearchForm>http://mycroft.mozdev.org/search-engines.html</SearchForm>
<os:Url type="text/html" method="GET" template="http://mycroft.mozdev.org/search-engines.html?name={searchTerms}">
</os:Url>
</SearchPlugin>